<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plansnet.hse.blackjack.Blackjack">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="dealerScore" text="Score: " />
            <Separator prefWidth="200.0" />
            <Label fx:id="dealerDeck" text="Label" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" text="Pass" />
            <Button mnemonicParsing="false" text="Get" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label fx:id="playerScore" text="Score: " />
            <Separator prefWidth="200.0" />
            <Label fx:id="playerDeck" text="Label" />
         </children>
      </HBox>
   </children>
</VBox>
